static inline int cvFloor( double value )
{

    __m128d t = _mm_set_sd( value );
    int i = _mm_cvtsd_si32(t);
    return i - _mm_movemask_pd(_mm_cmplt_sd(t, _mm_cvtsi32_sd(t,i)));
# 144 "/usr/local/include/opencv2/core/fast_math.hpp" 3
}
