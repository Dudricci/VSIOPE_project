# 27 "/home/alessandro/Scrivania/RoboCup/classifierCPP/cascadeClassifier.cpp" 2


# 28 "/home/alessandro/Scrivania/RoboCup/classifierCPP/cascadeClassifier.cpp"
using namespace cv;

int main( int argc, const char** argv )
{

    Mat image = imread("foto.jpg");


    CascadeClassifier face_cascade;
    if (!face_cascade.load("haarcascade_frontalface_alt.xml"))
    {
        std::cerr << "Failed to load face cascade classifier." << std::endl;
        return -1;
    }


    std::vector<Rect> faces;
    face_cascade.detectMultiScale(image, faces, 1.1, 2, 0|CASCADE_SCALE_IMAGE, Size(30, 30));


    for (size_t i = 0; i < faces.size(); i++)
    {
        rectangle(image, faces[i], Scalar(0, 255, 0), 2);
    }


    imshow("Detected Faces", image);
    waitKey();

    return 0;
}
