# 372 "/home/alessandro/Scrivania/RoboCup/opencv-3.4.1/modules/objdetect/include/opencv2/objdetect.hpp"
                   HOGDescriptor
{
public:
    enum { L2Hys = 0
         };
    enum { DEFAULT_NLEVELS = 64
         };




