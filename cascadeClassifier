# 4827 "/home/alessandro/Scrivania/RoboCup/opencv-3.4.1/modules/imgproc/include/opencv2/imgproc.hpp"
class 
# 4827 "/home/alessandro/Scrivania/RoboCup/opencv-3.4.1/modules/imgproc/include/opencv2/imgproc.hpp" 3
     __attribute__ ((visibility ("default"))) 
# 4827 "/home/alessandro/Scrivania/RoboCup/opencv-3.4.1/modules/imgproc/include/opencv2/imgproc.hpp"
                LineIterator
{
public:
# 4839 "/home/alessandro/Scrivania/RoboCup/opencv-3.4.1/modules/imgproc/include/opencv2/imgproc.hpp"
    LineIterator( const Mat& img, Point pt1, Point pt2,
                  int connectivity = 8, bool leftToRight = false );


    uchar* operator *();


    LineIterator& operator ++();


    LineIterator operator ++(int);


    Point pos() const;

    uchar* ptr;
    const uchar* ptr0;
    int step, elemSize;
    int err, count;
    int minusDelta, plusDelta;
    int minusStep, plusStep;
};





inline
uchar* LineIterator::operator *()
{
    return ptr;
}

inline
LineIterator& LineIterator::operator ++()
{
    int mask = err < 0 ? -1 : 0;
    err += minusDelta + (plusDelta & mask);
    ptr += minusStep + (plusStep & mask);
    return *this;
}

inline
LineIterator LineIterator::operator ++(int)
{
    LineIterator it = *this;
    ++(*this);
    return it;
}

inline
Point LineIterator::pos() const
{
    Point p;
    p.y = (int)((ptr - ptr0)/step);
    p.x = (int)(((ptr - ptr0) - p.y*step)/elemSize);
    return p;
}







}
