# 423 "/usr/local/include/opencv2/core/types_c.h" 3
typedef struct CvMat
{
    int type;
    int step;


    int* refcount;
    int hdr_refcount;

    union
    {
        uchar* ptr;
        short* s;
        int* i;
        float* fl;
        double* db;
    } data;


    union
    {
        int rows;
        int height;
    };

    union
    {
        int cols;
        int width;
    };







    CvMat() {}
    CvMat(const CvMat& m) { memcpy(this, &m, sizeof(CvMat));}
    CvMat(const cv::Mat& m);


}
CvMat;
# 509 "/usr/local/include/opencv2/core/types_c.h" 3
static inline CvMat cvMat( int rows, int cols, int type, void* data = __null)
{
    CvMat m;

    (static_cast <bool> ((unsigned)((type) & ((1 << 3) - 1)) <= 6) ? void (0) : __assert_fail ("(unsigned)CV_MAT_DEPTH(type) <= CV_64F", "/usr/local/include/opencv2/core/types_c.h", 513, __extension__ __PRETTY_FUNCTION__));
    type = ((type) & ((1 << 3)*512 - 1));
    m.type = 0x42420000 | (1 << 14) | type;
    m.cols = cols;
    m.rows = rows;
    m.step = m.cols*(((((type) & ((512 - 1) << 3)) >> 3) + 1) << ((((sizeof(size_t)/4+1)*16384|0x3a50) >> ((type) & ((1 << 3) - 1))*2) & 3));
    m.data.ptr = (uchar*)data;
    m.refcount = __null;
    m.hdr_refcount = 0;

    return m;
}


inline CvMat::CvMat(const cv::Mat& m)
{
    ;
    *this = cvMat(m.rows, m.dims == 1 ? 1 : m.cols, m.type(), m.data);
    step = (int)m.step[0];
    type = (type & ~cv::Mat::CONTINUOUS_FLAG) | (m.flags & cv::Mat::CONTINUOUS_FLAG);
}
# 557 "/usr/local/include/opencv2/core/types_c.h" 3
static inline double cvmGet( const CvMat* mat, int row, int col )
{
    int type;

    type = ((mat->type) & ((1 << 3)*512 - 1));
    (static_cast <bool> ((unsigned)row < (unsigned)mat->rows && (unsigned)col < (unsigned)mat->cols) ? void (0) : __assert_fail ("(unsigned)row < (unsigned)mat->rows && (unsigned)col < (unsigned)mat->cols", "/usr/local/include/opencv2/core/types_c.h", 562, __extension__ __PRETTY_FUNCTION__))
                                                 ;

    if( type == (((5) & ((1 << 3) - 1)) + (((1)-1) << 3)) )
        return ((float*)(void*)(mat->data.ptr + (size_t)mat->step*row))[col];
    else
    {
        (static_cast <bool> (type == (((6) & ((1 << 3) - 1)) + (((1)-1) << 3))) ? void (0) : __assert_fail ("type == CV_64FC1", "/usr/local/include/opencv2/core/types_c.h", 569, __extension__ __PRETTY_FUNCTION__));
        return ((double*)(void*)(mat->data.ptr + (size_t)mat->step*row))[col];
    }
}
# 584 "/usr/local/include/opencv2/core/types_c.h" 3
static inline void cvmSet( CvMat* mat, int row, int col, double value )
{
    int type;
    type = ((mat->type) & ((1 << 3)*512 - 1));
    (static_cast <bool> ((unsigned)row < (unsigned)mat->rows && (unsigned)col < (unsigned)mat->cols) ? void (0) : __assert_fail ("(unsigned)row < (unsigned)mat->rows && (unsigned)col < (unsigned)mat->cols", "/usr/local/include/opencv2/core/types_c.h", 588, __extension__ __PRETTY_FUNCTION__))
                                                 ;

    if( type == (((5) & ((1 << 3) - 1)) + (((1)-1) << 3)) )
        ((float*)(void*)(mat->data.ptr + (size_t)mat->step*row))[col] = (float)value;
    else
    {
        (static_cast <bool> (type == (((6) & ((1 << 3) - 1)) + (((1)-1) << 3))) ? void (0) : __assert_fail ("type == CV_64FC1", "/usr/local/include/opencv2/core/types_c.h", 595, __extension__ __PRETTY_FUNCTION__));
        ((double*)(void*)(mat->data.ptr + (size_t)mat->step*row))[col] = value;
    }
}


static inline int cvIplDepth( int type )
{
    int depth = ((type) & ((1 << 3) - 1));
    return ((((sizeof(size_t)<<28)|0x8442211) >> ((depth) & ((1 << 3) - 1))*4) & 15)*8 | (depth == 1 || depth == 3 ||
           depth == 4 ? 0x80000000 : 0);
}
